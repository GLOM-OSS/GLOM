{
  "name": "squoolr-v2-admin",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/squoolr-v2/admin",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/squoolr-v2/admin",
        "outputPath": "dist/apps/squoolr-v2/admin"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/squoolr-v2/admin"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "squoolr-v2-admin:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "squoolr-v2-admin:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "squoolr-v2-admin:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "squoolr-v2-admin:build:production"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/squoolr-v2/admin/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
