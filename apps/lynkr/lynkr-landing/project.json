{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "lynkr-landing",
  "sourceRoot": "apps/lynkr/lynkr-landing",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/lynkr/lynkr-landing",
        "outputPath": "dist/apps/lynkr/lynkr-landing"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/lynkr/lynkr-landing"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "lynkr-lynkr-landing:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "lynkr-lynkr-landing:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "lynkr-lynkr-landing:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "lynkr-lynkr-landing:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/lynkr/lynkr-landing"],
      "options": {
        "jestConfig": "apps/lynkr/lynkr-landing/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/lynkr/lynkr-landing/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
